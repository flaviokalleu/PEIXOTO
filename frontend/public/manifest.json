{
    "short_name": "MetaBot",
    "name": "MetaBot - Enterprise Solution",
    "version": "2.0.0",
    "lang": "pt-BR",
    "dir": "ltr",
    "icons": [
        {
            "src": "favicon.ico",
            "sizes": "64x64 32x32 24x24 16x16",
            "type": "image/x-icon",
            "purpose": "any"
        },
        {
            "src": "favicon-16x16.png",
            "sizes": "16x16",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "favicon-32x32.png",
            "sizes": "32x32",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "icon-144x144.png",
            "sizes": "144x144",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "apple-touch-icon.png",
            "sizes": "180x180",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "mstile-150x150.png",
            "sizes": "150x150",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "logo.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        }
    ],
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "orientation": "any",
    "theme_color": "#38BDF8",
    "background_color": "#0F172A",
    "prefer_related_applications": false,
    "categories": [
        "business",
        "productivity",
        "communication",
        "utilities"
    ],
    "description": "MetaBot - Plataforma Profissional de Atendimento Multicanal",
    "related_applications": [],
    "protocol_handlers": [
        {
            "protocol": "web+chat",
            "url": "/chat?id=%s"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400
    },
    "display_override": [
        "window-controls-overlay",
        "standalone",
        "browser"
    ],
    "handle_links": "preferred",
    "launch_handler": {
        "client_mode": ["focus-existing", "auto"]
    },
    "file_handlers": [
        {
            "action": "/upload",
            "accept": {
                "application/json": [".json"],
                "text/csv": [".csv"]
            }
        }
    ]
}